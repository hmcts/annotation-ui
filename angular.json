{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hmcts-annotation-ui-lib": {
      "root": "projects/hmcts-annotation-ui-lib",
      "sourceRoot": "projects/hmcts-annotation-ui-lib/src",
      "projectType": "library",
      "prefix": "aui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/hmcts-annotation-ui-lib/tsconfig.lib.json",
            "project": "projects/hmcts-annotation-ui-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/hmcts-annotation-ui-lib/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "codeCoverageExclude": [
              "**/*.spec.ts",
              "**/*.spec.js",
              "test/**",
              "config/**",
              "api/**/templates/**",
              "src/public/**",
              "src/environments/**"
            ],
            "main": "projects/hmcts-annotation-ui-lib/src/test.ts",
            "tsConfig": "projects/hmcts-annotation-ui-lib/tsconfig.spec.json",
            "karmaConfig": "projects/hmcts-annotation-ui-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/hmcts-annotation-ui-lib/tsconfig.lib.json",
              "projects/hmcts-annotation-ui-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hmcts-annotation-ui-app": {
      "root": "projects/hmcts-annotation-ui-app/",
      "sourceRoot": "projects/hmcts-annotation-ui-app/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hmcts-annotation-ui-app",
            "index": "projects/hmcts-annotation-ui-app/src/index.html",
            "main": "projects/hmcts-annotation-ui-app/src/main.ts",
            "polyfills": "projects/hmcts-annotation-ui-app/src/polyfills.ts",
            "tsConfig": "projects/hmcts-annotation-ui-app/src/tsconfig.app.json",
            "assets": [
              "projects/hmcts-annotation-ui-app/src/favicon.ico",
              "projects/hmcts-annotation-ui-app/src/assets",
              {
                "glob": "pdf.worker.js",
                "input": "../dist/hmcts-annotation-ui-lib/assets/javascripts/",
                "output": "./assets/"
              }
            ],
            "styles": [
              "projects/hmcts-annotation-ui-app/src/assets/annotation-ui-theme.scss",
              "projects/hmcts-annotation-ui-app/src/styles.css"
            ],
            "scripts": [
              "projects/hmcts-annotation-ui-lib/src/assets/javascripts/pdf.js",
              "projects/hmcts-annotation-ui-lib/src/assets/javascripts/pdf-worker.js",
              "projects/hmcts-annotation-ui-lib/src/assets/javascripts/pdf_viewer.js",
              "projects/hmcts-annotation-ui-lib/src/assets/javascripts/pdf-annotate.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/hmcts-annotation-ui-app/src/environments/environment.ts",
                  "with": "projects/hmcts-annotation-ui-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hmcts-annotation-ui-app:build",
            "port": 3000
          },
          "configurations": {
            "production": {
              "browserTarget": "hmcts-annotation-ui-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hmcts-annotation-ui-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "codeCoverageExclude": [
                "**/*.spec.ts",
                "**/*.spec.js",
                "test/**",
                "config/**",
                "api/**/templates/**",
                "projects/hmcts-annotation-ui-app/src/public/**",
                "projects/hmcts-annotation-ui-app/src/environments/**"
            ],
            "main": "projects/hmcts-annotation-ui-app/src/test.ts",
            "polyfills": "projects/hmcts-annotation-ui-app/src/polyfills.ts",
            "tsConfig": "projects/hmcts-annotation-ui-app/src/tsconfig.spec.json",
            "karmaConfig": "projects/hmcts-annotation-ui-app/src/karma.conf.js",
            "styles": [
              "projects/hmcts-annotation-ui-app/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/hmcts-annotation-ui-app/src/favicon.ico",
              "projects/hmcts-annotation-ui-app/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/hmcts-annotation-ui-app/src/tsconfig.app.json",
              "projects/hmcts-annotation-ui-app/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "hmcts-annotation-ui-app-e2e": {
      "root": "projects/hmcts-annotation-ui-app-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/hmcts-annotation-ui-app-e2e/protractor.conf.js",
            "devServerTarget": "hmcts-annotation-ui-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hmcts-annotation-ui-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/hmcts-annotation-ui-app-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "annotation-ui"
}
